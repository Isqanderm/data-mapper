{
  "name": "om-data-mapper",
  "version": "3.1.0",
  "description": "High-performance TypeScript/JavaScript data mapper with JIT compilation for ultra-fast object transformations",
  "private": false,
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "require": "./build/index.js",
      "default": "./build/index.js"
    },
    "./decorators": {
      "types": "./build/decorators/index.d.ts",
      "import": "./build/decorators/index.js",
      "require": "./build/decorators/index.js",
      "default": "./build/decorators/index.js"
    },
    "./class-transformer-compat": {
      "types": "./build/compat/class-transformer/index.d.ts",
      "import": "./build/compat/class-transformer/index.js",
      "require": "./build/compat/class-transformer/index.js",
      "default": "./build/compat/class-transformer/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm run clean && npm run test && npm run build",
    "release": "semantic-release",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "bench": "vitest bench --run",
    "bench:watch": "vitest bench",
    "bench:core": "vitest bench benchmarks/suites/core --run",
    "bench:compat:build": "cd benchmarks/suites/compat && tsc -p tsconfig.ct.json",
    "bench:compat": "npm run build && npm run bench:compat:build && node benchmarks/suites/compat/comparison.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Isqanderm/data-mapper.git"
  },
  "keywords": [
    "data mapping",
    "mapping",
    "mapper",
    "transform",
    "high performance",
    "jit",
    "fast",
    "mapping",
    "typescript",
    "javascript",
    "performance",
    "benchmark"
  ],
  "files": [
    "build",
    "README.md",
    "LICENSE"
  ],
  "author": "Isqanderm <aleksandr.melnik.personal@gmail.com> (https://www.linkedin.com/in/isqander-melnik)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Isqanderm/data-mapper/issues"
  },
  "homepage": "https://github.com/Isqanderm/data-mapper#readme",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^13.0.0",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/benchmark": "^2.1.5",
    "@vitest/coverage-v8": "^3.2.4",
    "benchmark": "^2.1.4",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "eslint": "^9.37.0",
    "prettier": "3.5.3",
    "reflect-metadata": "^0.2.2",
    "semantic-release": "^24.2.9",
    "tinybench": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}
