{
  "name": "om-data-mapper",
  "version": "2.0.5",
  "description": "High-performance TypeScript/JavaScript data mapper with JIT compilation for ultra-fast object transformations",
  "private": false,
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm run clean && npm run test && npm run build",
    "release": "semantic-release",
    "test": "vitest run --coverage",
    "test:jest": "jest",
    "test:watch": "vitest",
    "test:watch:jest": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Isqanderm/data-mapper.git"
  },
  "keywords": [
    "data mapping",
    "mapping",
    "mapper",
    "transform",
    "high performance",
    "jit",
    "fast",
    "mapping",
    "typescript",
    "javascript",
    "performance",
    "benchmark"
  ],
  "files": [
    "build",
    "README.md",
    "LICENSE"
  ],
  "author": "Isqanderm <aleksandr.melnik.personal@gmail.com> (https://www.linkedin.com/in/isqander-melnik)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Isqanderm/data-mapper/issues"
  },
  "homepage": "https://github.com/Isqanderm/data-mapper#readme",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@eslint/js": "^9.37.0",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/benchmark": "^2.1.5",
    "@types/jest": "^29.5.12",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-jest": "^29.7.0",
    "eslint": "^9.37.0",
    "jest": "^29.7.0",
    "prettier": "3.5.3",
    "semantic-release": "^24.2.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}
