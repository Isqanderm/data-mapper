{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "outDir": "./build-om",
    "rootDir": ".",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": false,
    "sourceMap": false,
    "baseUrl": ".",
    "paths": {
      "../../../../build/compat/class-transformer": ["../../../build/compat/class-transformer"]
    }
  },
  "include": ["models-om.ts"],
  "exclude": ["node_modules", "build-ct", "build-om"]
}

